@prefix : <http://mith.umd.edu/ema#> .
@prefix np:  <http://www.nanopub.org/nschema#> .
@prefix prov: <http://www.w3.org/ns/prov#> . 
@prefix oa: <http://www.w3.org/ns/oa#> .
@prefix dctypes: <http://purl.org/dc/dcmitype/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix cnt: <http://www.w3.org/2011/content#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

:DCnanopubEx {
  :DCnanopubEx a np:Nanopublication ;
    np:hasAssertion :assertion ;
    np:hasProvenance :provenance ;
    np:hasPublicationInfo :pubInfo .	    
}
  
:assertion {
  :annotation a oa:Annotation ;
    oa:hasBody :body ;
  # a tag could be used to structure the body or even replace it
  # oa:hasTag :tag ;
    oa:hasTarget :target ;
  #  oa:hasMotivation :motivation
    oa:annotatedBy :agent .
  :assertion a np:Assertion . 
}
  
:provenance {
  :assertion prov:wasAttributedTo :agent .
  :provenance a np:Provenance . 
}
  
:pubInfo {
  :DCnanopubEx prov:wasAttributedTo "EMAconversionsystem" .
  :pubInfo a np:PublicationInfo .
}

:target a oa:SpecificResource ;
  oa:hasSource <http://digitalduchemin.org/mei/DC0101.xml> ;
  oa:hasSelector :selector .

:body a dctypes:Text ;
  cnt:ContentAsText "Measure 33 has cross relation B-flat in Bass directly below a B(natural) in Ct" .

:agent a foaf:Person ;
  foaf:name "Quinn, Patrick" .

:selector a oa:FragmentSelector ;
#  dcterms:conformsTo <spec1> ;
  rdf:value "32-34" .